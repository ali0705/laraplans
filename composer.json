{
    "name": "ehsanrasta/laraplans",
    "description": "SaaS style recurring plans for Laravel.",
    "keywords": ["plans", "laravel", "subscriptions", "memberships"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "nesbot/carbon": "~2.0",
        "illuminate/support": "^7.0",
        "illuminate/database": "^7.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "orchestra/testbench": "~5.0",
        "mockery/mockery": "~1.3",
        "phpunit/phpunit": "~8.0"
    },
    "autoload": {
        "psr-4": {
            "EhsanRasta\\Laraplans\\": "src/Laraplans/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "EhsanRasta\\Laraplans\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}